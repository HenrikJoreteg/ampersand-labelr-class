{
  "name": "pnnl",
  "version": "1.9.0",
  "description": "An awesome app for managing labels",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "prebuild": "rm -rf public && mkdir public",
    "build": "NODE_ENV=production webpack",
    "deploy": "surge -p public -d pnnl.surge.sh",
    "commit": "git add --all && git commit -am \"$(date)\" && npm version minor && git push origin master --tags",
    "yolo": "npm run build && npm run deploy && npm run commit"
  },
