    "ampersand-router": "^3.0.2",
    "ampersand-view": "^7.3.0",
    "ampersand-view-switcher": "^2.0.0",
    "autoprefixer-stylus": "^0.6.0",
    "babel": "^5.2.16",
    "babel-core": "^5.2.16",
    "babel-loader": "^5.0.0",
    "css-loader": "^0.12.0",
    "file-loader": "^0.8.1",
    "hjs-webpack": "^2.1.0",
    "jade": "^1.9.2",
    "jade-loader": "^0.7.1",
    "local-links": "^1.4.0",
    "octicons": "^2.2.0",
    "qs": "^2.4.1",
